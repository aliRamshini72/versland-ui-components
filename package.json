{
  "version": "0.5.0",
  "description": "React components library for versland.io",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "versland-ui-components",
  "author": "Ali Ramshini",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aliRamshini72/versland-ui-components/"
  },
  "keywords": [
    "versland.io",
    "react"
  ],
  "module": "dist/versland-ui-components.esm.js",
  "size-limit": [
    {
      "path": "dist/versland-ui-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/versland-ui-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.15",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "classnames": "^2.3.2",
    "react-router-dom": "^6.8.1"
  }
}
