    {
      "path": "dist/versland-ui-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@ant-design/icons": "^5.0.1",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "cssnano": "^5.1.15",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.2.4",
    "tailwindcss": "^3.2.7",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^3.9.10"
  }
}
